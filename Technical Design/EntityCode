@startuml
entity User{
* userID: PK int, identity
--
* Name: varchar 255
--
*Email: varchar 255
--
*Password: varchar 255
}

entity Sales{
* salesID: PK int, identity
--
* userID: FK int
--
* Items
}

entity Inventory{
* inventoryID: PK int, identity
--
* inventoryName: varchar 255
--
* inventoryDescription: varchar 255
}

User||--{Sales
Inventory }|--|{ Sales

@enduml